<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx"
            fx:controller="br.com.parkineasy.view.controller.GerenteController">
    <top>
        <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="154.0" prefWidth="600.0"
                    BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="154.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../images/consulta-das-vagas.png"/>
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </top>
    <center>
        <AnchorPane prefHeight="327.0" prefWidth="600.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="btVoltarPainelVagas" layoutX="486.0" layoutY="411.0" mnemonicParsing="false"
                        onAction="#pressButtonConsultarVagas" prefHeight="27.0" prefWidth="100.0"
                        style="-fx-background-color: #2c344c;" text="Voltar" textFill="WHITE">
                    <font>
                        <Font size="13.0"/>
                    </font>
                </Button>
                <TableView fx:id="tableConsultarVagas" layoutY="-1.0" prefHeight="410.0" prefWidth="600.0"
                           style="-fx-background-color: #2c344c;">
                    <columns>
                        <TableColumn fx:id="colAConsultarVagas" editable="false" prefWidth="87.0" sortable="false"
                                     text="A"/>
                        <TableColumn fx:id="colBConsultarVagas" editable="false" prefWidth="87.0" sortable="false"
                                     text="B"/>
                        <TableColumn fx:id="colCConsultarVagas" editable="false" prefWidth="87.0" sortable="false"
                                     text="C"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </children>
        </AnchorPane>
    </center>
</BorderPane>
